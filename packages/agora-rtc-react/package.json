  "main": "src/main.ts",
  "publishConfig": {
    "main": "dist/main.js",
    "module": "dist/main.mjs",
    "types": "dist/main.d.ts"
  },
  "sideEffects": [
    "*.css"
  ],
  "source": "src/main.ts",
