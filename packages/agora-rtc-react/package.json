    "@types/react-dom": "^18.0.11",
    "@vitest/coverage-c8": "^0.29.2",
    "agora-rtc-sdk-ng": "^4.16.1",
    "jsdom": "^21.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
