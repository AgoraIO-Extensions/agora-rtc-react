{
  "name": "agora-rtc-react",
  "version": "0.0.1",
  "private": false,
  "description": "Agora RTC React SDK",
  "license": "MIT",
  "repository": "AgoraIO-Community/agora-rtc-react",
  "contributors": [
    "hyrious",
    "LichKing-2234",
