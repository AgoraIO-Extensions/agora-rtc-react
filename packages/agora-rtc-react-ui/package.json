    "build-storybook-docs": "storybook build --docs",
    "test": "vitest run --coverage",
    "test:watch": "vitest --ui",
    "version": "esbuild-dev ../../scripts/release/update-version.ts agora-rtc-react-ui"
  },
  "peerDependencies": {
    "agora-rtc-react": ">=2",
    "react": ">=16.8"
  },
  "devDependencies": {
    "agora-rtc-react": "workspace:*"
